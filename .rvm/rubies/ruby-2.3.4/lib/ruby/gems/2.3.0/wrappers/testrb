#!/usr/bin/env bash

if
  [[ -s "/home/travis/.rvm/gems/ruby-2.3.4@global/environment" ]]
then
  source "/home/travis/.rvm/gems/ruby-2.3.4@global/environment"
  exec testrb "$@"
else
  echo "ERROR: Missing RVM environment file: '/home/travis/.rvm/gems/ruby-2.3.4@global/environment'" >&2
  exit 1
fi
